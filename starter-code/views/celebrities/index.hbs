<h2>CELEBRITIES</h2>
{{#each celebrities}}
    <p>{{this.name}}</p>
    <a href="/celebrities/{{this._id}}">See details</a>
    <form action="/celebrities/{{this._id}}/delete" method="post">
    <button type="submit">DELETE</button>
    </form>
    <br><br>
    <form action="/celebrities/{{this._id}}/edit" method="post">
    <button type="submit">EDIT</button>
    </form>
{{/each}} 
<br>
<a href="/celebrities/new"> Add a celebrity </a>
